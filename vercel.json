{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    }
  ],
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "https://f1bnb-backend.vercel.app/api/$1",
      "statusCode": 307
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "index.js"
    }
  ]
}
